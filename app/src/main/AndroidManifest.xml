        <activity android:name="com.cosc2657.assignment3.MainActivity" />
        <activity android:name="com.cosc2657.assignment3.RegisterActivity" />
        <activity android:name="com.cosc2657.assignment3.LoginActivity" />
        <activity android:name="com.cosc2657.assignment3.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

